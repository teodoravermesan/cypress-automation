{
  "name": "cypress-automation",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "Teodora Vermesan",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "pretest": "npm run delete:reports",
    "delete:reports": "rm cypress/reports/* || true",
    "merge:reports": "mochawesome-merge  cypress/reports/mocha/*.json -o cypress/reports/report.json",
    "generate:report": "marge mochawesome.json --reportFilename mochawesomeReport.html",
    "test:junit": "cypress run --reporter junit --reporter-options mochaFile=results/Cypress/junit-[hash].xml",
    "test:mochawesome": "npm test && npm run merge:reports && npm run generate:report"
  },
  "devDependencies": {
    "cypress": "^13.17.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "mocha": "^11.0.1",
    "mochawesome": "^7.1.3",
    "multiple-cucumber-html-reporter": "^3.9.0"
  },
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    },
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/e2e/cucumber"
  },
  "dependencies": {
    "-": "^0.0.1",
    "save-dev": "^0.0.1-security"
  }
}
